{
	"name": "Xcode 11 Dark Purple Theme",
	"type": "dark",
	"colors": {
		"editor.background": "#211F27",
		"editor.foreground": "#eeffff",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb"
	},
    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#7f8c98"
            }
        },
        {
            "name": "String",
            "scope": [
                "string.quoted"
            ],
            "settings": {
                "foreground": "#fe8170"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "storage.type.function.arrow"
            ],
            "settings": {
                "foreground": "#ffffffd8"
            }
        },
        {
            "name": "Keyword",
            "scope": [
                "storage.type",
                "storage.modifier",
                "keyword",
                "variable.language.this",
                "variable.language.super",
                "keyword.other.typedef",
                "constant.language"
            ],
            "settings": {
                "foreground": "#fe7ab2",
                "fontStyle": "bold"
            }
        },
        {
            "name": "ClassDeclName",
            "scope": [
                "entity.name.type.class"
            ],
            "settings": {
                // "foreground": "#6bdfff",
                "foreground": "#ffffff"
            }
        },
        {
            "name": "InstanceProperties",
            "scope": [
                "variable.other.property",
                "variable.other.object.property"
            ],
            "settings": {
                "foreground": "#b181ec"
            }
        },
        {
            "name": "OtherDeclarations",
            "scope": [
                "entity.name.function",
                "entity.name.function.method"
            ],
            "settings": {
                "foreground": "#4db1cb"
            }
        },
        {
            "name": "ModuleNames",
            "scope": [
                "entity.name.type.module",
                "entity.other.inherited-class",
                "entity.name.class"
            ],
            "settings": {
                "foreground": "#d9baff"
            }
        },
        {
            "name": "TypeAnnotation",
            "scope": [
                "meta.type.annotation"
            ],
            "settings": {
                "foreground": "#6bdfff"
            }
        },
        {
            "name": "tag",
            "scope": [
                "punctuation.definition.tag",
                "entity.name.tag",
                "punctuation.separator.key-value"
            ],
            "settings": {
                "foreground": "#fe7ab2",
                "fontStyle": "bold"
            }
        },
        {
            "name": "JSSeparator",
            "scope": [
                "punctuation.separator.key-value.js"
            ],
            "settings": {
                "foreground": "#ffffffd9"
            }
        },
        {
            "name": "Attribute",
            "scope": [
                "meta.attribute",
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#cc9767"
            }
        },
        {
            "name": "Number",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#d8c87c"
            }
        },
        {
            "name": "Property",
            "scope": [
                "variable.other.property"
            ],
            "settings": {
                "foreground": "#abf2e4"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#ffffffd9"
            }
        },
        {
            "name": "Parameter",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#ffffffd9"
            }
        },
        {
            "name": "ClassType",
            "scope": [
                "support.type"
            ],
            "settings": {
                "foreground": "#d9baff"
            }
        },
        {
            "name": "Assignment",
            "scope": [
                "keyword.operator.assignment"
            ],
            "settings": {
                "foreground": "#ffffffd9"
            }
        },
        {
            "name": "GQLFragmentName",
            "scope": [
                "entity.name.fragment.graphql",
                "variable.fragment.graphql"
            ],
            "settings": {
                "foreground": "#6bdfff"
            }
        },
        {
            "name": "QuasiElement",
            "scope": [
                "punctuation.quasi.element"
            ],
            "settings": {
                "foreground": "#ffffffd9"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable.other.readwrite"
            ],
            "settings": {
                "foreground": "#ffffffd9"
            }
        },
        {
            "name": "Brace",
            "scope": [
                "meta.brace"
            ],
            "settings": {
                "foreground": "#ffffffd9"
            }
        },
        {
            "name": "Arithmetic",
            "scope": [
                "keyword.operator.arithmetic"
            ],
            "settings": {
                "foreground": "#b281eb"
            }
        },
        {
            "scope": "keyword - keyword.operator",
            "settings": {
                "foreground": "#FC5FA3"
            }
        },
        {
            "scope": "keyword.control",
            "settings": {
                "foreground": "#FC5FA3"
            }
        },
        {
            "scope": "storage",
            "settings": {
                "foreground": "#FC5FA3"
            }
        },
        {
            "scope": "storage.type",
            "settings": {
                "foreground": "#FC5FA3"
            }
        },
        {
            "scope": "entity.name.type",
            "settings": {
                "foreground": "#D0A8FF"
            }
        },
        {
            "scope": "entity.name.class",
            "settings": {
                "foreground": "#D0A8FF"
            }
        },
        {
            "scope": "support.type",
            "settings": {
                "foreground": "#D0A8FF"
            }
        },
        {
            "scope": "support.class",
            "settings": {
                "foreground": "#D0A8FF"
            }
        },
        {
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#A167E6"
            }
        },
        {
            "scope": "support.function",
            "settings": {
                "foreground": "#A167E6"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#67B7A4"
            }
        },
        {
            "scope": "entity.name.variable",
            "settings": {
                "foreground": "#67B7A4"
            }
        },
        {
            "scope": "token.info-token",
            "settings": {
                "foreground": "#6796e6"
            }
        },
        {
            "scope": "token.warn-token",
            "settings": {
                "foreground": "#cd9731"
            }
        },
        {
            "scope": "token.error-token",
            "settings": {
                "foreground": "#f44747"
            }
        },
        {
            "scope": "token.debug-token",
            "settings": {
                "foreground": "#b267e6"
            }
        }
    ]
}

